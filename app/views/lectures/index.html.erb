<h1>LMMS1</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Teacher</th>
      <th>Number</th>
      <th>Begin year</th>
      <th>Begin term</th>
      <th>Grade</th>
      <th>Materials path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lectures.each do |lecture| %>
      <tr>
        <td><%= lecture.name %></td>
        <td><%= lecture.teacher %></td>
        <td><%= lecture.number %></td>
        <td><%= lecture.begin_year %></td>
        <td><%= lecture.begin_term %></td>
        <td><%= lecture.grade %></td>
        <td><%= lecture.materials_path %></td>
        <td><%= link_to 'Show', lecture %></td>
        <td><%= link_to 'Edit', edit_lecture_path(lecture) %></td>
        <td><%= link_to 'Destroy', lecture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lecture', new_lecture_path %>
